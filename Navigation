Navigation Code
Cont. from 3/18/24

distance = brick.UltrasonicDist(4);
display(distance);

while 1
   distance = brick.UltrasonicDist(4);
   display(distance);
   brick.MoveMotor('A', 70);
   brick.MoveMotor('D', 70);
   
    if distance > 35
       brick.StopMotor('a');
       pause(1.85);
       brick.MoveMotor('a',70);
       pause(1.3);
       distance = brick.UltrasonicDist(4);
       display(distance);
    end
end
